{
  "name": "ssh2-they",
  "version": "2.1.0",
  "description": "Extends Mocha with the function `they` to transparently run tests in local and ssh mode",
  "keywords": [
    "ssh",
    "ssh2",
    "sftp",
    "secure",
    "test",
    "mocha"
  ],
  "homepage": "https://github.com/adaltas/node-ssh2-they",
  "license": "MIT",
  "author": "David Worms <david@adaltas.com> (https://www.adaltas.com)",
  "contributors": [],
  "coffeelintConfig": {
    "indentation": {
      "level": "error",
      "value": 2
    },
    "line_endings": {
      "level": "error",
      "value": "unix"
    },
    "max_line_length": {
      "level": "ignore"
    }
  },
  "engines": {
    "node": ">=0.10.1"
  },
  "main": "./lib/index",
  "mocha": {
    "throw-deprecation": true,
    "require": [
      "should",
      "coffeescript/register"
    ],
    "timeout": 40000,
    "reporter": "spec",
    "recursive": true
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/adaltas/node-ssh2-they.git"
  },
  "dependencies": {
    "ssh2-connect": "^3.0.0"
  },
  "devDependencies": {
    "coffeescript": "^2.5.1",
    "mocha": "^8.1.1",
    "should": "^13.2.3"
  },
  "optionalDependencies": {},
  "scripts": {
    "preversion": "grep '## Trunk' CHANGELOG.md",
    "version": "version=`grep '^  \"version\": ' package.json | sed 's/.*\"\\([0-9\\.]*\\)\".*/\\1/'` && sed -i \"s/## Trunk/## Version $version/\" CHANGELOG.md && git add CHANGELOG.md",
    "postversion": "git push && git push --tags && npm publish",
    "patch": "npm version patch -m 'Bump to version %s'",
    "minor": "npm version minor -m 'Bump to version %s'",
    "major": "npm version major -m 'Bump to version %s'",
    "build": "coffee -b -o lib src",
    "pretest": "npm run build",
    "test": "mocha 'test/**/*.coffee'"
  }
}
